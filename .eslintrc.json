{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {},
  "overrides": [
    {
      "files": ["*.ts"],
      "excludedFiles": "*.test.js",
      "rules": {
        "no-undef": "off"
      }
    }
  ]
}
